arr = list(map(int, input("Enter array elements: ").split()))

min_val = arr[0]
max_val = arr[0]

for num in arr:
    if num < min_val:
        min_val = num
    if num > max_val:
        max_val = num

print("Minimum:", min_val)
print("Maximum:", max_val)


num = int(input("Enter a number: "))
square = num * num
digit_sum = 0

while square > 0:
    digit_sum += square % 10
    square //= 10

if digit_sum == num:
    print("Neon Number")
else:
    print("Not a Neon Number")




